# ============================================
# Django Environment Configuration Example File
# ============================================

# Enable or disable Django debug mode (True/False)
DJANGO_DEBUG=

# Secret key for Django cryptographic signing
DJANGO_SECRET_KEY=

# Set the Django environment (e.g., production, development)
DJANGO_ENV=

# Comma-separated list of allowed hosts for the Django app
DJANGO_ALLOWED_HOST=


# ============================================
# PostgreSQL Database Settings
# ============================================

# Dababase provide
DATABASE=

# PostgreSQL database name
POSTGRES_DB=

# PostgreSQL username
POSTGRES_USER=

# PostgreSQL password
POSTGRES_PASSWORD=

# PostgreSQL host (e.g., localhost or db container name)
POSTGRES_HOST=

# PostgreSQL port (default: 5432)
POSTGRES_PORT=


# ============================================
# Django Project Bootstrap Settings
# ============================================

LOAD_FIXTURES=false

# Superuser (optional)
CREATE_SUPERUSER=false
DJANGO_SUPERUSER_USERNAME=
DJANGO_SUPERUSER_EMAIL=
DJANGO_SUPERUSER_PASSWORD=